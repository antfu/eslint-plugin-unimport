{
  "name": "eslint-plugin-unimport",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.11.0",
  "description": "Insert imports automatically from Unimport in ESLint.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/eslint-plugin-unimport#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/eslint-plugin-unimport.git"
  },
  "bugs": "https://github.com/antfu/eslint-plugin-unimport/issues",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "unimport"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^8.40.0"
  },
  "dependencies": {
    "@typescript-eslint/scope-manager": "^6.13.2",
    "@typescript-eslint/utils": "^6.13.2",
    "eslint-factory": "^0.0.2",
    "pathe": "^1.1.1",
    "unimport": "^3.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.3",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.6",
    "@types/eslint": "^8.44.8",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.4",
    "@vueuse/core": "^10.7.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.55.0",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-unimport": "workspace:*",
    "eslint-plugin-unused-imports": "^3.0.0",
    "esno": "^4.0.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.11.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.7",
    "vitest": "^1.0.2",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}

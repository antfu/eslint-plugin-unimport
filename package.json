{
  "name": "eslint-plugin-unimport",
  "type": "module",
  "version": "0.1.2",
  "packageManager": "pnpm@10.25.0",
  "description": "Insert imports automatically from Unimport in ESLint.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/eslint-plugin-unimport#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/eslint-plugin-unimport.git"
  },
  "bugs": "https://github.com/antfu/eslint-plugin-unimport/issues",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "unimport"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "test": "vitest",
    "typecheck": "tsc",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^9.0.0 || ^8.45.0"
  },
  "dependencies": {
    "@typescript-eslint/scope-manager": "^8.49.0",
    "@typescript-eslint/utils": "^8.49.0",
    "debug": "^4.4.3",
    "pathe": "^2.0.3",
    "unimport": "^5.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.0",
    "@antfu/ni": "^28.0.0",
    "@antfu/utils": "^9.3.0",
    "@types/debug": "^4.1.12",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.0.2",
    "@vueuse/core": "^14.1.0",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "eslint-plugin-unimport": "workspace:*",
    "execa": "^9.6.1",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "lint-staged": "^16.2.7",
    "pnpm": "^10.25.0",
    "publint": "^0.3.16",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "^0.17.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}

{
  "name": "eslint-plugin-unimport",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@9.12.3",
  "description": "Insert imports automatically from Unimport in ESLint.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/eslint-plugin-unimport#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/eslint-plugin-unimport.git"
  },
  "bugs": "https://github.com/antfu/eslint-plugin-unimport/issues",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "unimport"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "lint": "eslint --flag unstable_ts_config .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^9.0.0 || ^8.45.0"
  },
  "dependencies": {
    "@typescript-eslint/scope-manager": "^8.11.0",
    "@typescript-eslint/utils": "^8.11.0",
    "debug": "^4.3.7",
    "pathe": "^1.1.2",
    "unimport": "^3.13.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/debug": "^4.1.12",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.8.1",
    "@vueuse/core": "^11.1.0",
    "bumpp": "^9.8.0",
    "eslint": "^9.13.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-unimport": "workspace:*",
    "eslint-plugin-unused-imports": "^4.1.4",
    "esno": "^4.8.0",
    "execa": "^9.5.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "jiti": "^2.3.3",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.12.3",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.4",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --flag unstable_ts_config --fix"
  }
}

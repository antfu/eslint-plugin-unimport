{
  "name": "eslint-plugin-unimport",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.5",
  "description": "Insert imports automatically from Unimport in ESLint.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/eslint-plugin-unimport#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/eslint-plugin-unimport.git"
  },
  "bugs": "https://github.com/antfu/eslint-plugin-unimport/issues",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "unimport"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^8.40.0"
  },
  "dependencies": {
    "@typescript-eslint/scope-manager": "^7.4.0",
    "@typescript-eslint/utils": "^7.4.0",
    "eslint-factory": "^0.0.2",
    "pathe": "^1.1.2",
    "unimport": "^3.7.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.11.5",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/eslint": "^8.56.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.30",
    "@vueuse/core": "^10.9.0",
    "bumpp": "^9.4.0",
    "eslint": "npm:eslint-ts-patch@8.55.0-1",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-unimport": "workspace:*",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-ts-patch": "8.55.0-1",
    "esno": "^4.7.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.5",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.3",
    "unbuild": "^2.0.0",
    "vite": "^5.2.7",
    "vitest": "^1.4.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
